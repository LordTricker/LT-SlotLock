{
	"schemaVersion": 1,
	"id": "ltsl",
	"version": "${version}",
	"name": "LT-SlotLock",
	"description": "Amatorska modyfikacja pozwalająca na blokowanie slotów w ekiwpunku przed przypadkowym wyrzuceniem itemów. Jeżeli powielasz, to miłoby było jakbyś podzielił się info na kim bazujesz <3.",
	"authors": [
		"LordTricker"
	],
	"contact": {
		"homepage": "https://blazecode.pl/",
		"sources": "https://github.com/LordTricker"
	},
	"license": "MIT",
	"icon": "assets/ltsl/icon.png",

	"environment": "*",
	"entrypoints": {
		"client": [
			"pl.lordtricker.ltsl.client.LtslotlockClient"
		]
	},
	"mixins": [
		{
			"config": "ltsl.client.mixins.json",
			"environment": "client"
		}
	],

	"depends": {
		"fabricloader": ">=0.14.21",
		"minecraft": "~1.19.4",
		"java": ">=17",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}
